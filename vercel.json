{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": "vite",
  "env": {
    "VITE_ENV": "production"
  },
  "build": {
    "env": {
      "VITE_ENV": "production"
    }
  },
  "preview": {
    "env": {
      "VITE_ENV": "staging"
    }
  },
  "git": {
    "deploymentEnabled": {
      "main": true,
      "dev": true
    }
  },
  "regions": ["cpt1"],
  "functions": {
    "app/api/**/*.js": {
      "runtime": "nodejs18.x"
    }
  }
}