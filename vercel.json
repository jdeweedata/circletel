{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "functions": {
    "app/[uid]/page.tsx": {
      "maxDuration": 30,
      "memory": 512
    },
    "app/api/preview/route.ts": {
      "maxDuration": 15,
      "memory": 256
    },
    "app/admin/quotes/[id]/page.tsx": {
      "maxDuration": 60,
      "memory": 1024
    },
    "app/admin/quotes/[id]/edit/page.tsx": {
      "maxDuration": 60,
      "memory": 1024
    },
    "app/quotes/business/[id]/preview/page.tsx": {
      "maxDuration": 60,
      "memory": 1024
    },
    "app/api/quotes/business/list/route.ts": {
      "maxDuration": 60,
      "memory": 1024
    },
    "app/api/quotes/business/[id]/route.ts": {
      "maxDuration": 60,
      "memory": 1024
    },
    "app/api/quotes/business/[id]/email/route.ts": {
      "maxDuration": 60,
      "memory": 1024
    },
    "app/api/coverage/lead/route.ts": {
      "maxDuration": 15,
      "memory": 512
    },
    "app/api/cron/zoho-sync/route.ts": {
      "maxDuration": 600,
      "memory": 1024
    },
    "app/api/cron/integrations-health-check/route.ts": {
      "maxDuration": 60,
      "memory": 512
    },
    "app/api/cron/cleanup-webhook-logs/route.ts": {
      "maxDuration": 60,
      "memory": 512
    },
    "app/api/cron/competitor-scrape/route.ts": {
      "maxDuration": 300,
      "memory": 1024
    },
    "app/api/admin/competitor-analysis/scrape/route.ts": {
      "maxDuration": 300,
      "memory": 1024
    },
    "app/api/admin/competitor-analysis/test-scrape/route.ts": {
      "maxDuration": 120,
      "memory": 512
    },
    "app/api/cron/payment-sync-retry/route.ts": {
      "maxDuration": 120,
      "memory": 512
    },
    "app/api/cron/payment-sync-monitor/route.ts": {
      "maxDuration": 60,
      "memory": 512
    },
    "app/api/admin/integrations/ai-assistant/route.ts": {
      "maxDuration": 60,
      "memory": 512
    },
    "app/api/cron/submit-debit-orders/route.ts": {
      "maxDuration": 120,
      "memory": 512
    },
    "app/api/cron/submit-cc-debit-orders/route.ts": {
      "maxDuration": 120,
      "memory": 512
    },
    "app/api/admin/suppliers/[id]/sync/route.ts": {
      "maxDuration": 300,
      "memory": 1024
    },
    "app/api/cron/stats-snapshot/route.ts": {
      "maxDuration": 120,
      "memory": 512
    }
  },
  "crons": [
    {
      "path": "/api/cron/generate-invoices",
      "schedule": "0 0 * * *"
    },
    {
      "path": "/api/cron/expire-deals",
      "schedule": "0 2 * * *"
    },
    {
      "path": "/api/cron/price-changes",
      "schedule": "0 2 * * *"
    },
    {
      "path": "/api/cron/zoho-sync",
      "schedule": "0 0 * * *"
    },
    {
      "path": "/api/cron/integrations-health-check",
      "schedule": "*/30 * * * *"
    },
    {
      "path": "/api/cron/cleanup-webhook-logs",
      "schedule": "0 3 * * 0"
    },
    {
      "path": "/api/cron/competitor-scrape",
      "schedule": "0 1 * * *"
    },
    {
      "path": "/api/cron/payment-reconciliation",
      "schedule": "0 7 * * *"
    },
    {
      "path": "/api/cron/payment-sync-retry",
      "schedule": "0 */4 * * *"
    },
    {
      "path": "/api/cron/payment-sync-monitor",
      "schedule": "0 2,6,10,14,18,22 * * *"
    },
    {
      "path": "/api/cron/invoice-sms-reminders",
      "schedule": "0 8 * * *"
    },
    {
      "path": "/api/cron/ar-snapshot",
      "schedule": "0 21 * * *"
    },
    {
      "path": "/api/cron/submit-debit-orders",
      "schedule": "0 6 * * *"
    },
    {
      "path": "/api/cron/submit-cc-debit-orders",
      "schedule": "0 6 * * *"
    },
    {
      "path": "/api/cron/stats-snapshot",
      "schedule": "0 1 * * *"
    }
  ]
}