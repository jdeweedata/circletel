{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "crons": [
    {
      "path": "/api/cron/generate-invoices",
      "schedule": "0 0 * * *"
    },
    {
      "path": "/api/cron/expire-deals",
      "schedule": "0 2 * * *"
    }
  ]
}