# CircleTel Environment Variables Example
# Copy to .env.local for development

# Development Environment (Local Supabase)
VITE_SUPABASE_URL=http://127.0.0.1:54321
VITE_SUPABASE_ANON_KEY=your_local_supabase_anon_key

# Staging Environment (Copy to Vercel environment variables)
# VITE_SUPABASE_URL=https://staging-project-id.supabase.co
# VITE_SUPABASE_ANON_KEY=staging-anon-key

# Production Environment (Copy to Vercel environment variables)
# VITE_SUPABASE_URL=https://agyjovdugmtopasyvlng.supabase.co
# VITE_SUPABASE_ANON_KEY=production-anon-key

# Zoho CRM Integration
VITE_ZOHO_CLIENT_ID=your_zoho_client_id
VITE_ZOHO_CLIENT_SECRET=your_zoho_client_secret
VITE_ZOHO_REDIRECT_URI=your_redirect_uri

# Environment Identifier
NODE_ENV=development
VITE_ENV=development