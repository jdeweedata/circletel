{
  "cloudHostingPage": {
    "kind": "singleType",
    "collectionName": "cloud_hosting_pages",
    "info": {
      "singularName": "cloud-hosting-page",
      "pluralName": "cloud-hosting-pages",
      "displayName": "Cloud Hosting Page"
    },
    "attributes": {
      "hero": {
        "type": "component",
        "repeatable": false,
        "component": "sections.cloud-hero"
      },
      "features": {
        "type": "component",
        "repeatable": true,
        "component": "sections.feature-item"
      },
      "testimonial": {
        "type": "component",
        "repeatable": false,
        "component": "sections.testimonial-stats"
      },
      "pricingPlans": {
        "type": "component",
        "repeatable": true,
        "component": "sections.pricing-plan"
      },
      "performance": {
        "type": "component",
        "repeatable": false,
        "component": "sections.performance-section"
      },
      "awards": {
        "type": "component",
        "repeatable": true,
        "component": "sections.award-item"
      },
      "seo": {
        "type": "component",
        "repeatable": false,
        "component": "shared.seo"
      }
    }
  },
  "components": {
    "sections.cloud-hero": {
      "collectionName": "components_sections_cloud_heroes",
      "info": {
        "displayName": "Cloud Hero"
      },
      "attributes": {
        "title": {
          "type": "string",
          "required": true
        },
        "subtitle": {
          "type": "text"
        },
        "backgroundImage": {
          "type": "media",
          "allowedTypes": ["images"]
        }
      }
    },
    "sections.feature-item": {
      "collectionName": "components_sections_feature_items",
      "info": {
        "displayName": "Feature Item"
      },
      "attributes": {
        "icon": {
          "type": "string"
        },
        "title": {
          "type": "string",
          "required": true
        },
        "description": {
          "type": "string"
        }
      }
    },
    "sections.testimonial-stats": {
      "collectionName": "components_sections_testimonial_stats",
      "info": {
        "displayName": "Testimonial Stats"
      },
      "attributes": {
        "statNumber": {
          "type": "string"
        },
        "statText": {
          "type": "string"
        },
        "testimonialText": {
          "type": "text"
        },
        "testimonialAuthor": {
          "type": "string"
        },
        "testimonialRole": {
          "type": "string"
        }
      }
    },
    "sections.pricing-plan": {
      "collectionName": "components_sections_pricing_plans",
      "info": {
        "displayName": "Pricing Plan"
      },
      "attributes": {
        "planType": {
          "type": "enumeration",
          "enum": ["managed", "self-managed"]
        },
        "title": {
          "type": "string",
          "required": true
        },
        "description": {
          "type": "text"
        },
        "tiers": {
          "type": "component",
          "repeatable": true,
          "component": "shared.pricing-tier"
        }
      }
    },
    "shared.pricing-tier": {
      "collectionName": "components_shared_pricing_tiers",
      "info": {
        "displayName": "Pricing Tier"
      },
      "attributes": {
        "name": {
          "type": "string"
        },
        "cpu": {
          "type": "string"
        },
        "ram": {
          "type": "string"
        },
        "storage": {
          "type": "string"
        },
        "bandwidth": {
          "type": "string"
        },
        "price": {
          "type": "decimal"
        },
        "priceLabel": {
          "type": "string"
        },
        "isPopular": {
          "type": "boolean",
          "default": false
        }
      }
    },
    "sections.performance-section": {
      "collectionName": "components_sections_performance_sections",
      "info": {
        "displayName": "Performance Section"
      },
      "attributes": {
        "title": {
          "type": "string"
        },
        "subtitle": {
          "type": "string"
        },
        "description": {
          "type": "text"
        },
        "features": {
          "type": "component",
          "repeatable": true,
          "component": "shared.performance-feature"
        }
      }
    },
    "shared.performance-feature": {
      "collectionName": "components_shared_performance_features",
      "info": {
        "displayName": "Performance Feature"
      },
      "attributes": {
        "title": {
          "type": "string"
        },
        "description": {
          "type": "text"
        }
      }
    },
    "sections.award-item": {
      "collectionName": "components_sections_award_items",
      "info": {
        "displayName": "Award Item"
      },
      "attributes": {
        "title": {
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "year": {
          "type": "string"
        },
        "icon": {
          "type": "media",
          "allowedTypes": ["images"]
        }
      }
    }
  }
}