{
  "name": "circletel-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:memory": "node --max-old-space-size=8192 ./node_modules/next/dist/bin/next dev",
    "dev:low": "node --max-old-space-size=4096 ./node_modules/next/dist/bin/next dev",
    "build": "next build",
    "build:memory": "node --max-old-space-size=8192 ./node_modules/next/dist/bin/next build",
    "build:ci": "node --max-old-space-size=6144 ./node_modules/next/dist/bin/next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "type-check:memory": "node --max-old-space-size=4096 ./node_modules/typescript/bin/tsc --noEmit",
    "predeploy:check": "npm run type-check && npm run lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:payment": "jest __tests__/lib/payments",
    "test:types": "jest __tests__/lib/types",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:e2e:staging": "cross-env NEXT_PUBLIC_APP_URL=https://circletel-staging.vercel.app playwright test",
    "test:e2e:prod": "cross-env NEXT_PUBLIC_APP_URL=https://circletel.co.za playwright test --grep @smoke",
    "test:mobile": "playwright test --config=playwright.mobile.config.ts",
    "test:mobile:iphone": "playwright test --config=playwright.mobile.config.ts --project='iPhone 13'",
    "test:mobile:android": "playwright test --config=playwright.mobile.config.ts --project='Pixel 5'",
    "test:mobile:tablet": "playwright test --config=playwright.mobile.config.ts --project='iPad Mini'",
    "test:mobile:ui": "playwright test --config=playwright.mobile.config.ts --ui",
    "test:mobile:report": "playwright show-report playwright-report-mobile",
    "zoho:generate-skus": "npx tsx --tsconfig tsconfig.json scripts/generate-missing-skus.ts",
    "zoho:generate-skus:dry-run": "npx tsx --tsconfig tsconfig.json scripts/generate-missing-skus.ts --dry-run",
    "zoho:backfill-billing": "npx tsx --tsconfig tsconfig.json scripts/backfill-zoho-billing.ts",
    "zoho:backfill-billing:dry-run": "npx tsx --tsconfig tsconfig.json scripts/backfill-zoho-billing.ts --dry-run",
    "zoho:backfill": "npx tsx scripts/zoho-backfill-all.ts",
    "zoho:backfill:customers": "npx tsx scripts/zoho-backfill-customers.ts",
    "zoho:backfill:subscriptions": "npx tsx scripts/zoho-backfill-subscriptions.ts",
    "zoho:backfill:invoices": "npx tsx scripts/zoho-backfill-invoices.ts",
    "zoho:backfill:payments": "npx tsx scripts/zoho-backfill-payments.ts",
    "zoho:retry-failed": "npx tsx scripts/zoho-retry-failed-customers.ts",
    "zoho:health-check": "npx tsx scripts/zoho-health-check.ts",
    "zoho:alert-failed": "npx tsx scripts/zoho-alert-failed-syncs.ts",
    "statusline:update": "powershell -File .claude/update-ts-cache.ps1",
    "memory:check": "powershell -File scripts/check-memory.ps1",
    "memory:detail": "powershell -File scripts/check-memory-detailed.ps1",
    "workflow:start": "powershell -File scripts/workflow-morning.ps1",
    "workflow:start:auto": "powershell -File scripts/workflow-morning.ps1 -AutoCleanup",
    "workflow:cleanup": "powershell -File scripts/workflow-cleanup.ps1",
    "optimize:vscode": "powershell -File scripts/optimize-vscode.ps1",
    "optimize:vscode:apply": "powershell -File scripts/optimize-vscode.ps1 -ApplySettings",
    "optimize:windows": "powershell -File scripts/optimize-windows.ps1",
    "optimize:browser": "powershell -File scripts/optimize-browser.ps1",
    "optimize:all": "powershell -File scripts/optimize-dev-environment.ps1",
    "optimize:all:apply": "powershell -File scripts/optimize-dev-environment.ps1 -ApplyAll",
    "extensions:analyze": "powershell -File scripts/analyze-vscode-extensions.ps1",
    "vscode:force-cleanup": "powershell -File scripts/force-cleanup-vscode.ps1",
    "clean": "powershell -Command \"Remove-Item -Recurse -Force .next -ErrorAction SilentlyContinue\" && powershell -Command \"Remove-Item -Recurse -Force node_modules/.cache -ErrorAction SilentlyContinue\"",
    "analyze": "cross-env ANALYZE=true npm run build:memory",
    "orchestrate": "npx tsx scripts/run-orchestrator.ts",
    "slicemachine": "npx slice-machine-ui --port 9999"
  },
