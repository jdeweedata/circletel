# Staging Environment Configuration
# Copy this file to .env.staging and fill in the values

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://agyjovdugmtopasyvlng.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_staging_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_staging_service_role_key
SUPABASE_ACCESS_TOKEN=your_staging_access_token

# Strapi CMS (Optional - falls back to demo mode)
NEXT_PUBLIC_STRAPI_URL=http://localhost:1337
STRAPI_API_TOKEN=your_staging_strapi_token

# Google Maps API
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your_staging_google_maps_key

# Email Service (Resend)
RESEND_API_KEY=your_staging_resend_key

# Netcash Payment Gateway (Staging)
NETCASH_SERVICE_KEY=your_staging_netcash_key
NETCASH_MERCHANT_ID=your_staging_merchant_id
NETCASH_PAY_NOW_URL=https://sandbox.netcash.co.za/PayNow/PayNow.aspx
NETCASH_REDIRECT_URL=https://staging.circletel.vercel.app/order/success
NETCASH_NOTIFY_URL=https://staging.circletel.vercel.app/api/webhooks/netcash

# Zoho Integration
ZOHO_CLIENT_ID=your_staging_zoho_client_id
ZOHO_CLIENT_SECRET=your_staging_zoho_client_secret
ZOHO_REFRESH_TOKEN=your_staging_zoho_refresh_token
ZOHO_ORGANIZATION_ID=your_staging_zoho_org_id

# MTN Coverage API
MTN_BUSINESS_API_URL=https://staging-api.mtn.co.za
MTN_CONSUMER_API_URL=https://staging-consumer-api.mtn.co.za
MTN_API_KEY=your_staging_mtn_key

# Environment
NODE_ENV=staging
NEXT_PUBLIC_SITE_URL=https://staging.circletel.vercel.app

# Analytics
NEXT_PUBLIC_VERCEL_ANALYTICS_ID=your_staging_analytics_id