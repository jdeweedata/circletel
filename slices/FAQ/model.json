{
  "id": "faq",
  "type": "SharedSlice",
  "name": "FAQ",
  "description": "Frequently Asked Questions with accordion",
  "variations": [
    {
      "id": "default",
      "name": "Default",
      "docURL": "...",
      "version": "initial",
      "description": "Default",
      "imageUrl": "",
      "primary": {
        "section_title": {
          "type": "StructuredText",
          "config": {
            "label": "Section Title",
            "placeholder": "e.g., Frequently Asked Questions",
            "allowTargetBlank": false,
            "single": "heading2"
          }
        },
        "section_description": {
          "type": "StructuredText",
          "config": {
            "label": "Section Description",
            "placeholder": "Optional description text...",
            "allowTargetBlank": false,
            "single": "paragraph"
          }
        }
      },
      "items": {
        "question": {
          "type": "Text",
          "config": {
            "label": "Question",
            "placeholder": "FAQ question..."
          }
        },
        "answer": {
          "type": "StructuredText",
          "config": {
            "label": "Answer",
            "placeholder": "FAQ answer...",
            "allowTargetBlank": true,
            "multi": "paragraph,strong,em,hyperlink"
          }
        }
      }
    }
  ]
}
